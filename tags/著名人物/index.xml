  &lt;tbody&gt;
      &lt;tr&gt;
          &lt;td style=&#34;text-align: left&#34;&gt;&lt;strong&gt;阮海天&lt;/strong&gt; &lt;img src=&#34;http://localhost:1313/p/%E5%A2%9E%E5%9F%8E%E8%91%97%E5%90%8D%E4%BA%BA%E7%89%A9--%E7%BB%BC%E5%90%88%E5%AE%9E%E8%B7%B5%E8%AF%BE%E4%BD%9C%E4%B8%9A/%E5%A4%A9%E6%B5%B7.jpeg&#34;
	width=&#34;150&#34;
